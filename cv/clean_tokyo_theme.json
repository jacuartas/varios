{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "os",
          "style": "diamond",
          "background": "#FEF5ED",
          "foreground": "#011627",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#FEF5ED>\ue0b2</>",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}}",
          "properties": {
            "macos": "\uf179 ",
            "ubuntu": "\uf31b ",
            "windows": "\ue62a "
          }
        },
        {
          "type": "shell",
          "style": "diamond",
          "background": "#FEF5ED",
          "foreground": "#011627",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#FEF5ED>\ue0b2</>",
          "template": "\uf489 {{ .Name }}"
        },
        {
          "type": "session",
          "style": "diamond",
          "background": "#516BEB",
          "foreground": "#ffffff",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#516BEB>\ue0b2</>",
          "template": " \uf007 {{ .UserName }}@{{ .HostName }} "
        },
        {
          "type": "time",
          "style": "diamond",
          "background": "#7eb8da",
          "foreground": "#ffffff",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#7eb8da>\ue0b2</>",
          "template": " \uf073 {{ .CurrentDate | date .Format }} ",
          "properties": {
            "time_format": "15:04:05"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "sysinfo",
          "style": "diamond",
          "background": "#be9ddf",
          "foreground": "#ffffff",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#be9ddf>\ue0b2</>",
          "template": " \ue266 RAM: {{ round ((div ((sub .PhysicalTotalMemory .PhysicalAvailableMemory)|float64) 1073741824.0)) 1 }}/{{ round ((div .PhysicalTotalMemory 1073741824.0)) 1 }}GB "
        },
        {
          "type": "executiontime",
          "style": "diamond",
          "background": "#83769c",
          "foreground": "#ffffff",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#83769c>\ue0b2</>",
          "template": " \ufa1e {{ .FormattedMs }} ",
          "properties": {
            "threshold": 5000,
            "style": "austin"
          }
        },
        {
          "type": "battery",
          "style": "diamond",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}#40c4ff{{end}}",
            "{{if eq \"Discharging\" .State.String}}#ff5722{{end}}",
            "{{if eq \"Full\" .State.String}}#4caf50{{end}}"
          ],
          "foreground": "#ffffff",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent>\ue0b2</>",
          "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}% {{ end }}",
          "properties": {
            "discharging_icon": "\ue234 ",
            "charging_icon": "\ue235 ",
            "charged_icon": "\ue22f "
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "path",
          "style": "diamond",
          "background": "#ff9248",
          "foreground": "#ffffff",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#ff9248>\ue0b2</>",
          "template": " \uf07c {{ .Path }} ",
          "properties": {
            "style": "folder",
            "max_depth": 3,
            "folder_separator_icon": " \ue0b1 "
          }
        },
        {
          "type": "git",
          "style": "diamond",
          "background": "#2e9599",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#f36943{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#a8216b{{ end }}",
            "{{ if gt .Ahead 0 }}#35b5ff{{ end }}",
            "{{ if gt .Behind 0 }}#f89406{{ end }}"
          ],
          "foreground": "#ffffff",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#2e9599>\ue0b2</>",
          "template": " {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
          "properties": {
            "branch_max_length": 25,
            "fetch_status": true,
            "fetch_upstream_icon": true
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "node",
          "style": "diamond",
          "background": "#6CA35E",
          "foreground": "#ffffff",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#6CA35E>\ue0b2</>",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "properties": {
            "fetch_version": true,
            "fetch_package_manager": true,
            "npm_icon": " \ue71e ",
            "yarn_icon": " \uf61a "
          }
        },
        {
          "type": "python",
          "style": "diamond",
          "background": "#4B8BBE",
          "foreground": "#FFD43B",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#4B8BBE>\ue0b2</>",
          "template": " \uf820 {{ .Full }} ",
          "properties": {
            "display_mode": "files"
          }
        },
        {
          "type": "java",
          "style": "diamond",
          "background": "#4063D8",
          "foreground": "#ffffff",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#4063D8>\ue0b2</>",
          "template": " \ue738 {{ .Full }} ",
          "properties": {
            "display_mode": "files"
          }
        },
        {
          "type": "dotnet",
          "style": "diamond",
          "background": "#0e0e23",
          "foreground": "#0d7377",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#0e0e23>\ue0b2</>",
          "template": " \ue77f {{ .Full }} ",
          "properties": {
            "display_mode": "files"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "kubectl",
          "style": "diamond",
          "background": "#316ce4",
          "foreground": "#ffffff",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#316ce4>\ue0b2</>",
          "template": " \ufd31 {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}} ",
          "properties": {
            "display_mode": "files"
          }
        },
        {
          "type": "terraform",
          "style": "diamond",
          "background": "#4040B2",
          "foreground": "#ffffff",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#4040B2>\ue0b2</>",
          "template": " \ue20f {{ .WorkspaceName }} ",
          "properties": {
            "display_mode": "files"
          }
        },
        {
          "type": "aws",
          "style": "diamond",
          "background": "#faa029",
          "foreground": "#ffffff",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#faa029>\ue0b2</>",
          "template": " \ue7ad {{.Profile}}{{if .Region}}@{{.Region}}{{end}} ",
          "properties": {
            "display_mode": "files"
          }
        }
      ]
    },
    {
      "type": "rprompt",
      "alignment": "right",
      "segments": [
        {
          "type": "exit",
          "style": "diamond",
          "background": "#2e9599",
          "background_templates": [
            "{{ if gt .Code 0 }}#f1184c{{ end }}"
          ],
          "foreground": "#ffffff",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b4",
          "template": " {{ if gt .Code 0 }}\uf00d {{ .Meaning }}{{ else }}\uf42e{{ end }} ",
          "properties": {
            "always_enabled": true
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#007ACC",
          "template": "╰─"
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#007ACC",
          "template": " \u276f "
        }
      ]
    }
  ]
}